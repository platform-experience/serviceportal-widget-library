{
  "rules": {
    "comma-dangle": 2,
    "comma-spacing": 1,
    "default-case": 2,
    "eqeqeq": 2,
    "indent": [2, 2],
    "no-alert": 1,
    "no-console": 1,
    "no-debugger": 1,
    "no-duplicate-case": 2,
    "no-eval": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 0,
    "no-multi-spaces": 2,
    "no-new-func": 2,
    "no-octal": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-trailing-spaces": 2,
    "no-void": 0,
    "no-with": 2,
    "quotes": [1, "single"],
    "semi": ["error", "always"],
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "wrap-iife": [2, "inside"],
    "yoda": 2
  }
}