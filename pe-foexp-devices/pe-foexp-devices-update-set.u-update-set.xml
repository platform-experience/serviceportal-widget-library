    -ms-transform: rotate(210deg) translate(10em) rotate(-210deg);
    -o-transform: rotate(210deg) translate(10em) rotate(-210210);
    transform: rotate(210deg) translate(10em) rotate(-210deg);
  }

  .item:nth-child(2) {
