.circle-container {
  position: relative;
  width: 24em;
  height: 24em;
  padding: 2.8em; /*= 2em * 1.4 (2em = half the width of an img, 1.4 = sqrt(2))*/
  border-radius: 50%;
  margin: 0 auto;
  .circle {
    position: relative;
    background-color: $primary-color;
    border-radius: 50%;
    width: 100%;
    height: 100%;

    display: flex;
    justify-content: center;

    .fa-check-circle {
      display: none;
      position: absolute;
      font-size: 25px;
